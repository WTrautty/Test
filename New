        def get_bor_lan(ipv6_value):
            pattern = r'\b(?:[0-9a-fA-F]*:)?f::9/\d{1,3}\b'
            return re.sub(pattern, '0::/56', ipv6_value, flags=re.IGNORECASE)
        bor_lan = get_bor_lan(data.get("IPv6 WAN", ""))
        template = template.replace('{BOR LAN}',bor_lan)
        print(data.get(("IPv6 WAN", "")))
